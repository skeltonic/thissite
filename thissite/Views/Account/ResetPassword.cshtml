
@{
    ViewData["Title"] = "Reset Yer Password";
    
}
<div class="results titantext add-padd borders">
    <h2 class="header">Reset Your Password</h2>
</div>

<form method="post">    
    <div class="form-group">
        <label for="Password">
            Password
        </label>
        <input type="password" name="password" class="form-control" required placeholder="Enter your new password" />
    </div>
    <input type="submit" class="btn btn-primary" value="Set this as your password" />
</form>
